{
    "name": "nestjs-graphql-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "prepare-commit-msg": "devmoji -e",
            "commit-msg": "if git-branch-is dev; then commitlint -E HUSKY_GIT_PARAMS; fi"
        }
    },
    "scripts": {
        "prebuild": "rimraf dist && yarn run types",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "prestart": "yarn run types",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "types": "ts-node src/common/graphql/generator/type.generator.ts",
        "release": "standard-version",
        "release:major": "standard-version -- --release-as major",
        "release:minor": "standard-version -- --release-as minor",
        "release:patch": "standard-version -- --release-as patch",
        "release:version": "standard-version -- --release-as"
    },
    "dependencies": {
        "@juicycleff/nestjs-event-store": "^3.1.18",
        "@nestjs/common": "^8.2.6",
        "@nestjs/config": "^1.1.7",
        "@nestjs/core": "^8.2.6",
        "@nestjs/cqrs": "^8.0.1",
        "@nestjs/graphql": "^10.0.2",
        "@nestjs/platform-express": "^8.2.6",
        "@nestjs/swagger": "^5.2.0",
        "@nestjs/typeorm": "^8.0.3",
        "apollo-server-cache-redis": "^3.3.1",
        "apollo-server-express": "^3.6.3",
        "apollo-server-plugin-response-cache": "^3.5.1",
        "body-parser": "^1.19.1",
        "bs58": "^4.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cls-rtracer": "^2.6.0",
        "dotenv": "^16.0.0",
        "eosjs": "^22.1.0",
        "eosjs-ecc": "^4.0.7",
        "express-rate-limit": "^6.2.1",
        "graphql": "^16.3.0",
        "graphql-depth-limit": "^1.1.0",
        "graphql-rate-limit": "^3.3.0",
        "graphql-subscriptions": "^2.0.0",
        "graphql-tools": "^8.2.0",
        "graphql-type-json": "^0.3.2",
        "helmet": "^5.0.2",
        "ioredis": "^4.28.5",
        "jsonwebtoken": "^8.5.1",
        "key-encoder": "^2.0.3",
        "moment": "^2.29.1",
        "morgan": "^1.10.0",
        "nest-winston": "^1.6.2",
        "node-eventstore-client": "^0.2.18",
        "node-nats-streaming": "^0.3.2",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.4",
        "shamirs-secret-sharing-ts": "^1.0.2",
        "swagger-ui-express": "^4.3.0",
        "tsconfig-paths": "^3.12.0",
        "typeorm": "^0.2.41",
        "typeorm-transactional-cls-hooked": "^0.1.21",
        "winston": "^3.5.1",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.1.0",
        "@commitlint/config-conventional": "^16.0.0",
        "@nestjs/cli": "^8.2.0",
        "@nestjs/schematics": "^8.0.6",
        "@nestjs/testing": "^8.2.6",
        "@types/bs58": "^4.0.1",
        "@types/graphql-depth-limit": "^1.1.3",
        "@types/helmet": "^0.0.48",
        "@types/ioredis": "^4.28.8",
        "@types/jest": "27.4.0",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.17",
        "@types/supertest": "^2.0.11",
        "@types/xml2js": "^0.4.9",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "devmoji": "^2.3.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-security": "^1.4.0",
        "git-branch-is": "^4.0.0",
        "husky": "7.0.4",
        "jest": "27.5.1",
        "lint-staged": "^12.3.3",
        "prettier": "^2.5.1",
        "standard-version": "^9.3.2",
        "supertest": "^6.2.2",
        "ts-jest": "27.1.3",
        "ts-loader": "^9.2.6",
        "ts-node": "10.5.0",
        "typescript": "^4.5.5"
    },
    "packageManager": "yarn@3.1.1"
}
