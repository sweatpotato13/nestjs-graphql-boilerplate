{
    "name": "nestjs-graphql-boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "scripts": {
        "prebuild": "rimraf dist && yarn run types",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "prestart": "yarn run types",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "types": "ts-node src/common/graphql/generator/type.generator.ts",
        "release": "standard-version",
        "release:major": "standard-version -- --release-as major",
        "release:minor": "standard-version -- --release-as minor",
        "release:patch": "standard-version -- --release-as patch",
        "release:version": "standard-version -- --release-as"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/cqrs": "^7.0.1",
        "@nestjs/graphql": "^7.7.0",
        "@nestjs/platform-express": "^7.4.4",
        "@nestjs/swagger": "^4.6.1",
        "@nestjs/typeorm": "^7.1.4",
        "apollo-server-cache-redis": "^1.2.2",
        "apollo-server-express": "^2.18.2",
        "apollo-server-plugin-response-cache": "^0.5.5",
        "body-parser": "^1.19.0",
        "bs58": "^4.0.1",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "cls-rtracer": "^2.4.0",
        "dotenv": "^8.2.0",
        "eosjs": "^21.0.3",
        "eosjs-ecc": "^4.0.7",
        "express-rate-limit": "^5.1.3",
        "graphql": "^15.3.0",
        "graphql-depth-limit": "^1.1.0",
        "graphql-rate-limit": "^2.0.1",
        "graphql-subscriptions": "^1.1.0",
        "graphql-tools": "^6.2.4",
        "graphql-type-json": "^0.3.2",
        "helmet": "^3.23.3",
        "ioredis": "^4.17.3",
        "jsonwebtoken": "^8.5.1",
        "key-encoder": "^2.0.3",
        "moment": "^2.29.1",
        "morgan": "^1.10.0",
        "nest-winston": "^1.4.0",
        "node-eventstore-client": "^0.2.18",
        "node-nats-streaming": "^0.3.2",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "shamirs-secret-sharing-ts": "^1.0.2",
        "swagger-ui-express": "^4.1.4",
        "tsconfig-paths": "^3.9.0",
        "typeorm": "^0.2.28",
        "typeorm-transactional-cls-hooked": "^0.1.12",
        "winston": "^3.3.3",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/bs58": "^4.0.1",
        "@types/graphql-depth-limit": "^1.1.2",
        "@types/helmet": "^0.0.48",
        "@types/ioredis": "^4.17.5",
        "@types/jest": "26.0.10",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^13.9.1",
        "@types/supertest": "^2.0.8",
        "@types/xml2js": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-security": "^1.5.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "git-branch-is": "^4.0.0",
        "husky": "4.2.5",
        "jest": "26.4.2",
        "lint-staged": "^11.0.1",
        "prettier": "^2.1.2",
        "standard-version": "^9.3.1",
        "supertest": "^4.0.2",
        "ts-jest": "26.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "9.0.0",
        "typescript": "^4.7.4"
    },
    "packageManager": "yarn@3.2.2"
}
